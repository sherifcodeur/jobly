






{
    
    "manifest_version": 3,

    "name": "StreamZen",

    "version": "1.0",

    "description": "An extension to stop all the popups and false Links in Streaming Website",


    "permissions": ["activeTab","contextMenus","tabs","scripting"],
    "host_permissions": [
    	"<all_urls>"
  	],

    "content_scripts":[{
        "matches":["<all_urls>"],
        "js":["content.js"]
        }],

        "background": {
            "service_worker": "background.js"
          },
    
    "action" : { 
        "default_icon" : "images/streamzen128.png"
    },
    "icons": {
        "16": "images/streamzen16.png",
        "32": "images/streamzen32.png",
        "48": "images/streamzen48.png",
        "128":"images/streamzen128.png"
    }}